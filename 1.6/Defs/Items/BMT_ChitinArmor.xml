<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>BMT_Apparel_ChitinLightArmor</defName>
		<label>chitin light armor</label>
		<description>Overlapping chitin plates of armor only covering shoulders and legs.</description>
		<possessionCount>1</possessionCount>
		<graphicData>
			<texPath>BMT_Caverns/Things/Apparel/ChitinLightArmor/ChitinLightArmor</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<techLevel>Medieval</techLevel>
		<tradeability>Sellable</tradeability>
		<generateCommonality>0.2</generateCommonality>
		<costList>
			<Steel>10</Steel>
		</costList>
		<stuffCategories>
			<li>BMT_ChitinStuff</li>
		</stuffCategories>
		<costStuffCount>100</costStuffCount>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToMake>10000</WorkToMake>
			<Mass>8</Mass>
			<StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.14</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.0</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<tradeTags>
			<li>Clothing</li>
			<li>Armor</li>
		</tradeTags>
		<apparel>
			<bodyPartGroups>
				<li>Shoulders</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>BMT_Caverns/Things/Apparel/ChitinLightArmor/ChitinLightArmor</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Shell</li>
				<li>Middle</li>
			</layers>
			<tags>
				<li>MedievalMilitary</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
			<researchPrerequisite>PlateArmor</researchPrerequisite>
			<skillRequirements>
				<Crafting>6</Crafting>
			</skillRequirements>
			<recipeUsers>
				<li>CraftingSpot</li>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
	</ThingDef>

