<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="PlantBase">
		<defName>BMT_BiolumiAlgaeCarnelian</defName>
		<label>bioluminescence algae</label>
		<description>A bioluminescence algae that grows in underground water.</description>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Nutrition>0.5</Nutrition>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
			<Flammability>0.0</Flammability>
		</statBases>
		<graphicData>
			<graphicClass>Graphic_Random</graphicClass>
			<texPath>BMT_Caverns/Things/Plant/BioluminescenceAlgae/Carnelian</texPath>
		</graphicData>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>2.9</glowRadius>
				<glowColor>(179, 27, 27, 0)</glowColor>
			</li>
		</comps>
		<modExtensions>
			<li Class="BiomesCore.DefModExtensions.Biomes_PlantControl">
				<terrainTags>
					<li>Water</li>
					<li>Fresh</li>
					<li>Shallow</li>
					<li>Boggy</li>
					<li>Marshy</li>
				</terrainTags>
			</li>
		</modExtensions>
		<hideAtSnowOrSandDepth>0.5</hideAtSnowOrSandDepth>
		<plant>
			<fertilityMin>0.15</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>3.5</growDays>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.4~0.65</visualSizeRange>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>350</wildClusterWeight>
			<wildOrder>2</wildOrder>
			<growMinGlow>0</growMinGlow>
			<growOptimalGlow>0</growOptimalGlow>
			<interferesWithRoof>false</interferesWithRoof>
		</plant>
	</ThingDef>
