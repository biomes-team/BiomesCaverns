<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<ModMetaData>
  <name>Biomes! Caverns</name>
  <author>The Biomes Mod Team</author>
  <supportedVersions>
    <li>1.4</li>
    <li>1.5</li>
  </supportedVersions>
  <packageId>BiomesTeam.BiomesCaverns</packageId>
  <modIconPath IgnoreIfNoMatchingField="True">BMT_Caverns/UI/Caverns_Logo</modIconPath>
  <modDependencies>
    <li>
      <packageId>brrainz.harmony</packageId>
      <displayName>Harmony</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
      <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
    </li>
    <li>
      <packageId>BiomesTeam.BiomesCore</packageId>
      <displayName>Biomes! Core</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2038000893</steamWorkshopUrl>
    </li>
    <li>
      <packageId>m00nl1ght.GeologicalLandforms</packageId>
      <displayName>Geological Landforms</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2773943594</steamWorkshopUrl>
      <downloadUrl>https://github.com/m00nl1ght-dev/GeologicalLandforms/releases/latest</downloadUrl>
    </li>
  </modDependencies>
  <loadBefore>
    <!--
    If the "Lag Free Lamps" option is enabled, Biomes! Caverns will fail to apply its dark torch patches if it loads
    after Tweaks Galore.
    -->
    <li>Neronix17.TweaksGalore</li>
  </loadBefore>
  <loadAfter>
    <li>Ludeon.RimWorld</li>
    <li>Ludeon.RimWorld.Royalty</li>
    <li>Ludeon.RimWorld.Ideology</li>
    <li>Ludeon.RimWorld.Biotech</li>
    <li>brrainz.harmony</li>
    <li>BiomesTeam.BiomesCore</li>
    <li>BiomesTeam.BiomesIslands</li>
    <li>m00nl1ght.GeologicalLandforms</li>
    <li>VanillaExpanded.VCEF</li>
    <li>dankpyon.medieval.overhaul</li>
  </loadAfter>
  <incompatibleWith>
    <!-- <li>fluffysminion.itw.caveworldfloraunleashed</li> -->
    <!-- [KV] Impassable Map Maker overrides Biomes! Caverns map generation with their own. -->
    <li>impassablemapmaker.kv.rw</li>
  </incompatibleWith>
  <description>The caverns module for the Biomes! mods. Requires Biomes! Core loaded before it.</description>
</ModMetaData>