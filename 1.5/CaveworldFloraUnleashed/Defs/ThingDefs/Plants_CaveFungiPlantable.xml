<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- ========== EDIBLE MUSHROOMS ========== -->
	
					<!-- SHIMMERSHROOM -->
	<!-- A rare wild mushroom that can be harvested for a foodstuff that is better to eat raw -->
	<ThingDef ParentName="BMT_CavePlantBase">
		<defName>BMT_ShimmershroomPlanted</defName>
		<label>shimmershroom</label>
		<description>Some soft and delicate caveworld shimmershrooms. Much appreciated for their subtle taste they are considered a delicacy by caveworlds inhabitants. It can only be grown in fungiponics.</description>
		<statBases>
			<MaxHitPoints>25</MaxHitPoints>
			<Flammability>0.4</Flammability>
			<Beauty>5</Beauty>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/BMT_Shimmershroom</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.3, 0.3, 0.2)</volume>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<fillPercent>0.10</fillPercent>
		<pathCost>15</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<growMinGlow>0</growMinGlow>
			<growOptimalGlow>0</growOptimalGlow>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<fertilityMin>0.1</fertilityMin>
			<wildClusterRadius>7</wildClusterRadius>
			<wildClusterWeight>250</wildClusterWeight>
			<topWindExposure>0.1</topWindExposure>
			<harvestMinGrowth>0.90</harvestMinGrowth>
			<harvestWork>50</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>BMT_RawShimmershroom</harvestedThingDef>
			<harvestYield>5</harvestYield>
			<visualSizeRange>0.6~0.8</visualSizeRange>
			<growDays>10</growDays>
			<sowTags>
				<li>BMT_Fungiponics</li>
			</sowTags>
			<sowResearchPrerequisites>
				<li>BMT_ResearchMushrooms</li>
			</sowResearchPrerequisites>
			<sowMinSkill>7</sowMinSkill>	  
			<wildOrder>0</wildOrder>
		  <purpose>Food</purpose>
		  <humanFoodPlant>true</humanFoodPlant>			
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>3</glowRadius>
				<glowColor>(75,0,128,0)</glowColor>
			</li>
		 </comps>
	</ThingDef>
  
					<!-- GLOWBULB -->
	<!-- Grows tougher fruiting bodies than most cave fungi -->
	<ThingDef ParentName="BMT_CavePlantBase">
		<defName>BMT_GlowbulbPlanted</defName>
		<label>glowbulb</label>
		<description>A small glowing bulb, found growing in nearly every corner of dark caves. While technically edible, its very rough taste make it better used as a cooked ingredient. It can only be grown in fungiponics.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Flammability>0.4</Flammability>
			<Beauty>2</Beauty>
			<Nutrition>0.20</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/BMT_Glowbulb</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.3, 0.4, 0.3)</volume>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<fillPercent>0.15</fillPercent>
		<pathCost>30</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<growMinGlow>0</growMinGlow>
			<growOptimalGlow>0</growOptimalGlow>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<fertilityMin>0.1</fertilityMin>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>250</wildClusterWeight>
			<topWindExposure>0.1</topWindExposure>
			<harvestMinGrowth>0.90</harvestMinGrowth>
			<harvestWork>50</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>BMT_RawGlowbulb</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<visualSizeRange>0.6~0.8</visualSizeRange>
			<growDays>14</growDays>
			<sowTags>	
				<li>BMT_Fungiponics</li>
			</sowTags>
			<sowResearchPrerequisites>
				<li>BMT_ResearchMushrooms</li>
			</sowResearchPrerequisites>	  
			<wildOrder>0</wildOrder>
      <purpose>Food</purpose>
      <humanFoodPlant>true</humanFoodPlant>			
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>4</glowRadius>
				<glowColor>(30,221,166,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
