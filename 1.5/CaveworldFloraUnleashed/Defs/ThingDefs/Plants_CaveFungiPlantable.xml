					<!-- BRIGHTBELLS -->
	<!-- Beautiful, can make mining much more tolerable -->
	<ThingDef ParentName="BMT_CavePlantBase">
		<defName>BMT_BrightbellsPlanted</defName>
		<label>brightbells</label>
		<description>A very elegant fungus, appreciated by miners for the light emitted by the little bulbs ending its fragile branches. It is often used by caveworld people as a natural lightning; its fibrous texture renders it inedible though. It can only be grown in fungiponics.</description>
		<statBases>
			<MaxHitPoints>15</MaxHitPoints>
			<Flammability>0.3</Flammability>
			<Beauty>20</Beauty>
			<Nutrition>0.10</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/BMT_Brightbells</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.3, 0.3, 0.3)</volume>
			</shadowData>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<selectable>true</selectable>
