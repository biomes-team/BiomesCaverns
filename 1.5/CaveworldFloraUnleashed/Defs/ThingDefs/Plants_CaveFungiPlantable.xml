	<!-- ========== OTHER MUSHROOMS ========== -->

					<!-- GLEAMCAP -->
	<!-- Caveworld herbal medicine -->
	<ThingDef ParentName="BMT_CavePlantBase">
		<defName>BMT_GleamcapPlanted</defName>
		<label>gleamcap</label>
		<description>A massive caveworld mushroom, known for its natural healing properties by caveworld tenders. Gleamcap spores are psycoactive and are released whenever the mushroom is disturbed. It can only be grown in fungiponics.</description>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<Flammability>0.3</Flammability>
			<Beauty>4</Beauty>
			<Nutrition>0.40</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/BMT_Gleamcap</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.5, 0.5, 0.4)</volume>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<fillPercent>0.20</fillPercent>
		<pathCost>15</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<growMinGlow>0</growMinGlow>
			<growOptimalGlow>0</growOptimalGlow>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<fertilityMin>0.1</fertilityMin>
