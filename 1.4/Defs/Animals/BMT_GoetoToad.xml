<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="AnimalThingBase">
		<defName>BMT_Goeto</defName>
		<label>goeto toad</label>
		<description>The Goeto Toad is another attempt at creating a meat and egg source for mining exportation centers and underground colonies. The project rapidly got out of hand and the specimens quickly adapted to the caves, becoming its new 'natural' habitat.</description>
		<statBases>
			<MoveSpeed>2.1</MoveSpeed>
			<SwimSpeed MayRequire="biomesteam.biomesislands">13.6</SwimSpeed>
			<MarketValue>50</MarketValue>
			<ComfyTemperatureMin>-10</ComfyTemperatureMin>
			<ComfyTemperatureMax>50</ComfyTemperatureMax>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>3</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>1</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggUnfertilizedDef>BMT_EggGoetoUnfertilized</eggUnfertilizedDef>
				<eggFertilizedDef>BMT_EggGoetoFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>5</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>5~6</eggCountRange>
			</li>
		</comps>
		<race>
			<body>QuadrupedAnimalWithPaws</body>
			<baseBodySize>0.3</baseBodySize>
			<baseHealthScale>0.35</baseHealthScale>
			<baseHungerRate>0.2</baseHungerRate>
			<foodType>OmnivoreRoughAnimal</foodType>
			<leatherDef>BMT_Leather_Amphibian</leatherDef>
			<useMeatFrom>BMT_Maxolotl</useMeatFrom>
			<wildness>0</wildness>
			<trainability>None</trainability>
			<roamMtbDays>5</roamMtbDays>
			<mateMtbHours>8</mateMtbHours>
			<lifeExpectancy>10</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBabyTiny</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.12</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.2</minAge>
					<soundWounded>Pawn_Iguana_Wounded</soundWounded>
					<soundDeath>Pawn_Iguana_Death</soundDeath>
					<soundCall>Pawn_Iguana_Call</soundCall>
					<soundAngry>Pawn_Iguana_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalFarm</li>
		</tradeTags>
	</ThingDef>
